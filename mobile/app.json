{
  "expo": {
    "scheme": "acme",
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Permitir que $(PRODUCT_NAME) acesse suas fotos.",
          "savePhotosPermission": "Permitir que $(PRODUCT_NAME) salve fotos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "name": "Geladinho Santista",
    "slug": "geladinho-santista",
    "android": {
      "package": "com.geladinhosantista.app",
      "icon": "./assets/logo.png",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION",
        "android.permission.INTERNET"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "34aaa80b-50f2-47a1-a9d1-2ed988e58361"
      }
    }
  }
}
